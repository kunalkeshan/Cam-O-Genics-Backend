	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kunalkeshan/Cam-O-Genics-Backend/issues"
	},
	"homepage": "https://github.com/kunalkeshan/Cam-O-Genics-Backend#readme",
	"dependencies": {
		"@wavychat/mjml-parser": "^1.2.0",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"date-fns": "^2.29.2",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"handlebars": "^4.7.7",
		"joi": "^17.6.0",
		"jsonwebtoken": "^8.5.1",
		"mjml": "^4.13.0",
		"mongoose": "^6.5.3",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
